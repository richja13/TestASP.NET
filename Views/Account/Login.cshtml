@model TestWebApp.Models.Login

<h2>Logowanie</h2>

@using (Html.BeginForm("Login", "Account", FormMethod.Post))
{
    @Html.ValidationSummary()
    ;
    <span>Login: </span> @Html.TextBoxFor(x => x.UserName)<br>
    <span>Password: </span> @Html.PasswordFor(x => x.Password)<br>

    <button type="submit">Zaloguj</button>
    <br>
}

<p>@ViewData["Error"]</p>